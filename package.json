{
  "name": "serverless-scotty",
  "version": "0.1.3",
  "description": "Scotty helps you to accelerate your serverless development with AWS SAM. Scotty updates your Lambda code without redeploying the whole stack.",
  "main": "index.js",
  "bin": {
    "scotty": "./src/index.js"
  },
  "scripts": {
    "scotty": "BUCKET_NAME=<bucket name> REGION=<region> node ./src/index.js"
  },
  "keywords": [
    "serverless",
    "aws sam",
    "deployment",
    "lambda",
    "framework"
  ],
  "author": "Dimitri Tarasowski",
  "license": "MIT",
  "dependencies": {
    "archiver-promise": "^1.0.0",
    "aws-sdk": "^2.338.0",
    "js-yaml": "^3.12.0"
  },
  "devDependencies": {
    "tape": "^4.9.1"
  }
}